+        InputStream stream = Bukkit.class.getClassLoader().getResourceAsStream("META-INF/maven/ultravanilla.paper/ultrapaper-api/pom.properties"); // UltraPaper
         Properties properties = new Properties();
 
         if (stream != null) {
